{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes" : [
        {
            "src": "/api/trades",
            "dest":"index.js"
        },
        {
            "src": "/api/trades/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/" 
        }
      
    ]
  }
  